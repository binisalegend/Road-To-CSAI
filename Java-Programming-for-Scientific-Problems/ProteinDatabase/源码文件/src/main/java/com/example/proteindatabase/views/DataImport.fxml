   <Button fx:id="FileChooseButton" alignment="CENTER" layoutX="280.0" layoutY="52.0" mnemonicParsing="false" onAction="#onFileChooseButtonClick" prefHeight="23.0" prefWidth="69.0" text="浏览文件" />
   <TextField fx:id="LabelNameTextField" layoutX="467.0" layoutY="52.0" prefHeight="23.0" prefWidth="182.0" promptText="请输入要建立的数据表名称" />
   <Label layoutX="406.0" layoutY="51.0" prefHeight="23.0" prefWidth="57.0" text="数据表名" textAlignment="CENTER" />
